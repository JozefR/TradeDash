@using TradeDash.FrontEnd.ViewModels
@model IEnumerable<TradeDash.FrontEnd.ViewModels.Strategy.StrategyViewModel>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<main role="main" class="col-md-11 ml-sm-auto col-lg-11 px-4">
    <div class="row">
        <div class="col-6">
            <h4 style="text-align: center">Strategies</h4>
            <table class="table table-striped table-sm table-bordered">
                <thead>
                <tr>
                    <th>Name</th>
                    <th>Strategy Type</th>
                </tr>
                </thead>
                <tbody>
                @foreach (var strategyViewModel in Model)
                {
                    <tr>
                        <td>@strategyViewModel.Name</td>
                        <td>@strategyViewModel.StrategyType</td>
                    </tr>
                }
                </tbody>
            </table>
        </div>
    </div>
</main>